{{#if row.isExpandable}}
    {{#if row.expanded}}
        {{#if row.isLoading}}
            {{fa-icon 'spinner' pulse=true fixedWidth=true}}
        {{else}}
            {{fa-icon 'caret-down' fixedWidth=true}}
        {{/if}}
    {{else}}
        {{fa-icon 'caret-right' fixedWidth=true}}
    {{/if}}
{{/if}}
{{fa-icon iconName fixedWidth=true}}
